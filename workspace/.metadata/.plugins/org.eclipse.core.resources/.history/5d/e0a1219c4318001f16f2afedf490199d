package ch02.sec05;

public class BoleanExample {

	public static void main(String[] args) {
		boolean stop = ture;
		if (stop) {
			System.out.println("중지합니다.");

		} else {
			System.out.println("시작합니다.");
		}

		int x = 10;
		boolean result1 = (x == 20);	// 변수 x의 값이 20 인가?
		boolean result2 = (x != 20);	// 변수 x의 값이 20이 아닌가?
		System.out.println("rseult1 " + result1);
		System.out.println("resutl2 " + result2);
	}

}
