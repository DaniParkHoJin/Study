package homework;

public class GuGuDanGaRo {

	public static void main(String[] args) {
		System.out.println("                                구구단");

		for (int i = 2; i < 10; i++) {
			System.out.print(i + "단/t");
		}
		System.out.println();

		for (int j = 2; j < 10; j++) {
			System.out.print(j + "*1=" + (j * 1) + "/t");

		}

	}

}
