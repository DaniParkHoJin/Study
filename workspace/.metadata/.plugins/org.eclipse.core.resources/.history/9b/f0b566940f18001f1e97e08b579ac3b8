package homework;

public class GuGuDanGaRo {

	public static void main(String[] args) {
		System.out.println("                                구구단");

		for (int i = 2; i < 10; i++) {
			System.out.printf("%d단\t\t", i);
		}
		System.out.println();
		int i, j;
		i = 2;
		j = 1;
		for (i = 2; i < 10; i++) {
			System.out.printf("%d * %d = %d\t", i, j, (i * j));
			for (j = 1; j < 10; j++) {
			}
			System.out.print(j);
		}

		System.out.println();

	}

}
