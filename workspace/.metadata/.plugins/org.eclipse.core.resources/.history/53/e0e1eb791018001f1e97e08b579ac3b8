package homework;

public class GuGuDanGaRo {

	public static void main(String[] args) {
		System.out.println("                                구구단");

		for (int i = 2; i < 10; i++) {
			System.out.printf("%d단\t\t", i);
		}
		System.out.println();
		for (int i = 2; i < 10; i++) {
			System.out.print(i + " * 1 = " + (i*1)+ "\t");
			
		}
	}

	}


