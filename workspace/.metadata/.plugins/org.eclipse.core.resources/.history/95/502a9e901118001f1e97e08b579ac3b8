package homework;

public class GuGuDanGaRo {

	public static void main(String[] args) {
		System.out.println("                                구구단");

		for (int i = 2; i < 10; i++) {
			System.out.printf("%d단\t\t", i);
		}
		System.out.println();

for(int j = 2; j < 10; j++) {
	for (int i = 2; i < 10; i++) {
		System.out.print(j + " * 1 = " + (j*i)+ "\t");
	}	
	System.out.println();
}
		
	
		
	}

}
