package homework;

public class GuGuDanGaRo {

	public static void main(String[] args) {
		System.out.println("                                구구단");

		for (int i = 2; i < 10; i++) {
			System.out.printf("%d단\t\t",i);	
		}
		System.out.println();
		for (int i = 2; i < 10; i++) {
			for (int j = 1 ; j < 10; j++) {

			System.out.printf("%d * %d = %d\t\t", i, j, (i*j));
				
			}
		}
		
		System.out.println();


	}

}
