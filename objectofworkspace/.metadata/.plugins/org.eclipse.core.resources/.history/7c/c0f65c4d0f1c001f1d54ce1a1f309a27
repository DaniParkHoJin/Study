
public class TestCourse {

	public static void main(String[] args) {

			Course course1 = new Course("자료구조");
			Course course2 = new Course("데이터 베이스");
			
			course1.addStudent("홍길동");
			course1.addStudent("이순신");
			course1.addStudent("강감찬");
			course2.addStudent("홍길동");
			course2.addStudent("김유신");
			
			System.out.println("course1 의 학생 수 : " + course1.getNumberOfStudent());
			String[] students = course1.getStudents();
			
			for (int i = 0; i < course1.getNumberOfStudent() ; i++) {
				System.out.println(students[i] + ", ");
			}
			System.out.println();
			System.out.println("course2 의 학생 수 : " + course2.getNumberOfStudent());
	}

}
